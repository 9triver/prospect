//风险管理
entity ProjectRisk {
	id String,
    	year Long,
    	nodename String,
    	risktype Integer,
    	decumentid Long,
    	version Integer,
    	usetime LocalDate,
    	systemlevel Integer,
    	risklevel Risklevel,
    	limitationtime String,
    	closetype Integer
}
//风险报告
entity RiskReport{
	id String,
    	type String,
    	riskreportname String,
    	releasetime LocalDate,
    	auditStatus AuditStatus
}
//风险级别
enum Risklevel{
	One, Two, Three, Four, Five, Six
}

relationship ManyToMany{
	ProjectRisk{Projectwbs} to Projectwbs,
	ProgressPlan{ProjectRisk} to ProjectRisk
}
relationship ManyToOne{
	ProjectRisk{RiskReport} to RiskReport,
	ProjectRisk{creatorid} to Officers,
	ProjectRisk{responsibleperson} to Officers,
	ProjectRisk{auditorid} to Officers,
	RiskReport{creatorid} to Officers,
	RiskReport{auditorid} to Officers
}


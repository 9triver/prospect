//进度计划
entity ProgressPlan {
	id String,
    planname String,
	belongproject String,
	belongplanid String,
    secretlevel Secretlevel,
	plantype Integer,
	planlevel PlanLevel,
	planstage String,
	readytime LocalDate,
	description String,
	deliverables String,
	planobjectives String,
	preplan String,
    starttime LocalDate,
    endtime LocalDate,
    actualstarttime LocalDate,
    actualendtime LocalDate,
	progress Integer,
    progresstype Progressstatus,
	iskey Integer,
    status Planstatus,
    auditStatus AuditStatus,
	returns String,
	remark String
}
enum Planstatus {
	IN_DEADLINE, OVERDUE
}
enum PlanLevel {
	CYCLE, STAGE, YEAR, MONTH, WEEK
}
enum Progressstatus{
	/**进度计划状态：未启动，启动，结束*/
    NOT_START, START, COMPLETED
}
relationship ManyToMany{
	ProgressPlan{Projectwbs} to Projectwbs{ProgressPlan},
	ProgressPlan{ProjectRisk} to ProjectRisk{ProgressPlan}
}
relationship ManyToOne{
	ProgressPlan{responsibleperson} to HrManagement,
	ProgressPlan{cooperatingperson} to HrManagement,
	ProgressPlan{auditorid} to HrManagement,
	ProgressPlan{responsibledepartment} to Department,
	ProgressPlan{cooperatingdepartment} to Department
}
//计划回报
entity PlanReturns {
	id String,
    planreturnsname String,
	plantype Integer,
	planlevel PlanLevel,
	description String,
    actualstarttime LocalDate,
    actualendtime LocalDate,
	deliverables String,
	progress Integer,
    status Planstatus,
	impactanalysis String,
    returnstime LocalDate,
	rejectionreason String,
    returnsstatus ReturnsStatus
}
relationship OneToMany{
	ProgressPlan{PlanReturns} to PlanReturns
}
relationship ManyToOne{
	PlanReturns{responsibleperson} to HrManagement,
	PlanReturns{auditorid} to HrManagement
}
enum ReturnsStatus{
	/**回报状态：未启动，提前完成，正常进行未完成,正常进行未完成,
	正常进行已完成,延期未完成,延期已完成,催办未完成,催办已完成*/
    NOT_STARTED,
	COMPLETE_AHEAD_OF_SCHEDULE,
	NORMAL_PROGRESS_INCOMPLETE,
	NORMAL_PROGRESS_COMPLETED,
	DELAY_NOT_COMPLETED,
	DELAY_COMPLETED,
	REMINDER_NOT_COMPLETED,
	REMINDER_HAS_COMPLETED
}
//承沿合同信息
entity Contract{
	id Integer,
	contractcode String,
	contractname String,
	projectid String,
	projectname String,
	contracttype ContractType,
	year Integer,
	amount BigDecimal,
	starttime LocalDate,
	endtime LocalDate,
	secretlevel Secretlevel,
	status ContractStatus,
	//预算金额
	budgetamount BigDecimal,
	//估算金额
	estimatedamount BigDecimal,
	//已实施金额
	implementedamount BigDecimal,
	//差额
	difference BigDecimal
}
//合同类型
enum ContractType{
	/**合同类型：采购合同，销售合同，服务合同，租赁合同，其他合同*/
    PURCHASE_CONTRACT,
	SALES_CONTRACT,
	SERVICE_CONTRACT,
	RENTAL_CONTRACT,
	OTHER_CONTRACT
}
//合同状态
enum ContractStatus{
	/**合同状态：未生效，已生效，已完成，已终止，已作废*/
    NOT_EFFECTIVE,
	EFFECTIVE,
	COMPLETED,
	TERMINATED,
	INVALID
}

/**
 * 人员  //add entity
 * @author A true hipster
 */
entity Officers {
	id String,
	name String required,
    password String,
    email String,
    phone Long,
	hiredate LocalDate,
	years Integer,
	status OfficersStatus
}
enum OfficersStatus{
	ON_JOB, NO_JOB, JOB_WAITION
}
//人力资源管理
entity HrManagement{
	id Integer,
	officersid String,
    officersname String,
	projectid Integer,
	projectname String,
	//项目角色
	projectrole String,
	//岗位等级
	jobgrade String,
	departmentid String,
	departmentname String,
	//所属战线
	frontlineid String,
	frontlinename String,
	//岗位职责
	jobduty String,
	//年度工时投入（人天）
	annualworktime Integer,
	//年度任务目标
	annualtasktarget String
}
relationship ManyToOne{
	HrManagement{officers} to Officers{hrmanagement}
}
//部门
entity Department{
	id String,
    name String,
    description String,
	role String,
    officersnum Integer
}
relationship ManyToMany{
	Officers{departments} to Department{officers}
}
relationship ManyToOne{
	//上级部门
	Department{superior} to Department
}
//战线
entity Frontline{
	id String,
    name String,
    description String
}
relationship ManyToMany{
	Officers{frontline} to Frontline{officers}
}
//角色
entity Role{
	id String,
    rolename String,
    description String
}
//权限
entity Permission{
	id String,
    permissionname String,
    description String
}
relationship ManyToMany{
	Officers{role} to Role,
	Role{permission} to Permission
} 
enum Secretlevel {
	/**密级：公开，内部，秘密，机密*/
    PUBLIC,
	INTERNAL,
	SECRET,
	CONFIDENTIAL
}
enum ProjectStatus {
	/**项目状态：未开始、进行中、已完成、已取消、已超期**/
    NOTSTART, IN_PROGRESS, COMPLETED, CANCELED, EXPIRED
}
enum AuditStatus{
	/**审核状态：未审核、审核中、已通过**/
    NOT_AUDITED, IN_AUDIT, APPROVED
}

